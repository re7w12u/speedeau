                Template="Ribbon.Templates.REF.CustomTemplate">
                <Controls Id="Ribbon.EDFTab.REF.CustomGroup.Controls">
                  <Button
                    Id="Ribbon.EDFTab.REF.CustomGroup.UploadDocument"
