      Rights="AddListItems"
      RegistrationType="ContentType">
    <CommandUIExtension>
      <CommandUIDefinitions>
        <CommandUIDefinition
          Location="Ribbon.Tabs._children">
          <Tab
            Id="Ribbon.EDFTab.REF"
            Title="Fonctions"
            Description="This holds EDF specific commands"
            Sequence="10001">
            <Scaling
              Id="Ribbon.EDFTab.REF.Scaling">
              <MaxSize
